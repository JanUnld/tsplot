{% extends 'mmd/base.njk' -%}
{% set diagramType = 'classDiagram' -%}

{% block content %}
  {% for member in graph.members %}
    {% block member %}
      {% include 'mmd/member.njk' %}
    {% endblock %}
  {% endfor %}
  {% for edge in graph.edges %}
    {% block edge %}
      {% include 'mmd/edge.njk' %}
    {% endblock %}
  {% endfor %}
{% endblock %}
