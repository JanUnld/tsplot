{% extends 'tsplot/plant-uml/base.njk' -%}

{% block content %}
  {% for namespace in projectView.namespaces %}
    {% block namespace %}
      {% if namespace.members.length > 0 %}
        {% include 'tsplot/plant-uml/namespace.njk' %}
      {% endif %}
    {% endblock %}
  {% endfor %}
  {% for member in projectView.orphans %}
    {% block orphan %}
      {% include 'tsplot/plant-uml/member.njk' %}
    {% endblock %}
  {% endfor %}
  {% for edge in projectGraph.edges %}
    {% block edge %}
      {% include 'tsplot/common/edge.njk' %}
    {% endblock %}
  {% endfor %}
{% endblock %}
