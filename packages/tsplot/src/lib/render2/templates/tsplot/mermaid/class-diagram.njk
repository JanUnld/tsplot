{% extends 'tsplot/mermaid/base.njk' -%}
{% set diagramType = 'classDiagram' -%}

{% block content %}
  {% for namespace in projectView.namespaces %}
    {% block namespace %}
      {% include 'tsplot/mermaid/namespace.njk' %}
    {% endblock %}
  {% endfor %}
  {% for member in projectView.orphans %}
    {% block orphan %}
      {% include 'tsplot/mermaid/member.njk' %}
    {% endblock %}
  {% endfor %}
  {% for edge in projectGraph.edges %}
    {% block edge %}
      {% include 'tsplot/mermaid/edge.njk' %}
    {% endblock %}
  {% endfor %}
{% endblock %}
